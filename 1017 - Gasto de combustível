#include <stdio.h>
#include <stdlib.h>

int main(void) {
    
    int tempo, velocidade;
    double distancia, combustivel_gasto;
    
    scanf("%d", &tempo);
    scanf("%d", &velocidade);
    
    distancia = tempo * velocidade;
    combustivel_gasto = distancia / 12;
    
    printf("%.3lf\n", combustivel_gasto);
    
}
